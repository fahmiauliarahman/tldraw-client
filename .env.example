# Copy this file to .env and adjust values as needed

# Production app port
TLDRAW_CLIENT_PORT=3046

# Development app port  
TLDRAW_DEV_PORT=5173

# WebSocket server port for real-time collaboration
WEBSOCKET_PORT=1234

# Node environment
NODE_ENV=production

# Nginx worker processes (optional)
NGINX_WORKER_PROCESSES=auto

# Nginx worker connections (optional)
NGINX_WORKER_CONNECTIONS=1024
